<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:layout1="sap.ui.commons.layout" controllerName="generated.app.controller.Sidebar">
    <m:Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false">
        <m:content>
            <layout1:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout1:positions>
                    <layout1:PositionContainer left="0px" top="0px">
                        <layout1:control>
                            <m:VBox class="sideBar" alignItems="Stretch" direction="Column" fitContainer="false" width="300px" height="1027px" justifyContent="Start" renderType="Div" visible="true" displayInline="false" backgroundDesign="Solid">
                                <m:items>
                                    <form:Form width="100%" editable="true" visible="true">
                                        <form:title/>
                                        <form:formContainers>
                                            <form:FormContainer visible="true">
                                                <form:formElements>
                                                    <form:FormElement visible="true">
                                                        <form:label>
                                                            <m:Label text="Tipo de Reunião" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <m:ComboBox placeholder="Seleccione um tipo de reunião" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%">
                                                                <m:items>
                                                                    <core:Item text="Formação" enabled="true"/>
                                                                    <core:Item text="Interna" enabled="true"/>
                                                                    <core:Item text="Externa" enabled="true"/>
                                                                </m:items>
                                                            </m:ComboBox>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement visible="true">
                                                        <form:label>
                                                            <m:Label text="Seleção de Datas e Horas (Início)" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <m:DateTimeInput placeholder="DD/MM/YYYY, HH:mm" type="DateTime" enabled="true" visible="true" width="auto"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement visible="true">
                                                        <form:label>
                                                            <m:Label text="Seleção de Datas e Horas (Fim)" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <m:DateTimeInput placeholder="DD/MM/YYYY, HH:mm" type="DateTime" enabled="true" visible="true" width="auto"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement visible="true">
                                                        <form:label>
                                                            <m:Label design="Bold" width="0" required="false" textAlign="Begin" textDirection="Inherit"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <m:RadioButton selected="false" text="Manhã" editable="true" enabled="true" visible="true" width="auto" valueState="None"/>
                                                            <m:RadioButton selected="false" text="Tarde" editable="true" enabled="true" visible="true" width="auto" valueState="None"/>
                                                            <m:RadioButton selected="false" text="Dia" editable="true" enabled="true" visible="true" width="auto" valueState="None"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                    <form:FormElement visible="true">
                                                        <form:label>
                                                            <m:Label text="Nº de Participantes" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
                                                        </form:label>
                                                        <form:fields>
                                                            <m:Input value="" placeholder="Insira o número de participantes esperado" type="Number" description="" showValueHelp="false" enabled="true" visible="true" width="auto" valueHelpOnly="false"/>
                                                        </form:fields>
                                                    </form:FormElement>
                                                </form:formElements>
                                                <form:title/>
                                            </form:FormContainer>
                                        </form:formContainers>
                                        <form:layout>
                                            <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                                        </form:layout>
                                    </form:Form>
                                    <layout:Grid defaultIndent="L1 M0 S0" defaultSpan="L2 M4 S12" position="Center" width="100%" containerQuery="true" hSpacing="1" vSpacing="1">
                                        <layout:content>
                                            <m:ToggleButton pressed="true" text="Laser" icon="sap-icon://target-group" iconFirst="true" width="auto" enabled="true" visible="true"/>
                                            <m:ToggleButton pressed="true" text="Microfone" icon="sap-icon://microphone" iconFirst="true" width="auto" enabled="true" visible="true"/>
                                            <m:ToggleButton pressed="true" text="Flipchart" icon="sap-icon://sys-monitor" iconFirst="true" width="auto" enabled="true" visible="true"/>
                                            <m:ToggleButton pressed="true" text="Material de Escritório" icon="sap-icon://edit" iconFirst="true" width="auto" enabled="true" visible="true"/>
                                            <m:ToggleButton pressed="true" text="Projetor" icon="sap-icon://projector" iconFirst="true" width="auto" enabled="true" visible="true"/>
                                        </layout:content>
                                    </layout:Grid>
                                    <m:HBox alignItems="Center" direction="Row" fitContainer="false" width="100%" height="auto" justifyContent="Center" renderType="Div" visible="true" displayInline="false">
                                        <m:items>
                                            <m:Button text="Alterar Dados" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                                        </m:items>
                                        <m:layoutData/>
                                    </m:HBox>
                                    <m:HBox alignItems="Center" direction="Row" fitContainer="false" width="100%" height="auto" justifyContent="Center" renderType="Div" visible="true" displayInline="false">
                                        <m:items>
                                            <m:Image src="/webapp/resources/logo.png" height="150px" width="200px" densityAware="false"/>
                                        </m:items>
                                        <m:layoutData/>
                                    </m:HBox>
                                </m:items>
                                <m:layoutData/>
                            </m:VBox>
                        </layout1:control>
                    </layout1:PositionContainer>
                </layout1:positions>
            </layout1:AbsoluteLayout>
        </m:content>
        <m:footer/>
        <m:headerContent/>
        <m:customHeader/>
    </m:Page>
</mvc:View>