<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:core="sap.ui.core" viewName="helpSection" controllerName="odkasfactory.reservasalas.controller.HelpSection">
	<m:Page class="help_page" showHeader="false" title="Responsive Page Title" showFooter="false" showNavButton="false">
		<m:content>
			<m:HBox id="help_header" class="header">
				<m:Title class="title" text="Como efetuar uma Reserva?" titleStyle="H1"  visible="true"/>
				<core:Icon class="icon" src="sap-icon://decline" size="30px" color="red" height="30px" activeColor="red" visible="true" press="_onClosePressed"/>
			</m:HBox>
			<m:HBox alignItems="Center" direction="Row" fitContainer="true" width="auto" height="auto" justifyContent="Center" renderType="Div" visible="true" displayInline="false">
				<m:items>
					<m:Image class="image" src="/webapp/images/help1.png" />
				</m:items>
				<m:layoutData/>
			</m:HBox>
		</m:content>
		<m:footer/>
		<m:headerContent/>
		<m:subHeader/>
		<m:customHeader/>
	</m:Page>
</mvc:View>